{
  "name": "react-lit",
  "version": "0.1.0",
  "description": "This project was bootstrapped with Create React App.",
  "author": "Yingray <yingray.lu@fuhu.com>",
  "scripts": {
    "start": "npm run watch-css & REACT_APP_MODE=dev react-scripts start",
    "build": "npm run build-css && REACT_APP_MODE=dev react-scripts build",
    "watch-css": "node-sass --recursive src/styles/sass -o src/styles -w",
    "build-css": "node-sass --recursive src/styles/sass -o src/styles",
    "test": "react-scripts test --env=jsdom"
  },
  "devDependencies": {
    "babel-polyfill": "^6.16.0",
    "lodash": "^4.17.2",
    "node-sass": "^3.13.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.6",
    "react-scripts": "0.8.4",
    "redux": "^3.6.0",
    "redux-mock-store": "^1.2.1",
    "redux-thunk": "^2.1.0"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "path": "^0.12.7"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/yingray/react-lit.git"
    }
  ],
  "keywords": [
    "react",
    "redux",
    "yarn",
    "docker"
  ],
  "license": "MIT"
}
